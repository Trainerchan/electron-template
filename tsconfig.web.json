{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "lib/main.ts"
  ],
  "exclude": [
    "src/**/__tests__/*",
    // "src/main.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/renderer/*"
      ]
    },
    "types": []
  }
}